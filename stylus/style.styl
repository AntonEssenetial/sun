// Preloader
.loader-wrapper
  width: 100%;
  height: 100%;
  position:fixed;
  z-index: 111;
  top: 0px;
  left: 0px;
.loader {
  position: absolute;
  top: 50%;
  left: 50%;
  margin: -15px 0 0 -175px;
  transform: scale(0.75);
  z-index 112
}
.loader:hover {
    cursor: wait;
}
.loader .dot {
  z-index: 3;
  position: relative;
  float: left;
  width: 50px;
  height: 50px;
  text-align: center;
  content: '';
  background: #fff;
  color: blue;
  font: lighter 20px/50px "Pt Sans"
  opacity: 0;
  animation: loader 5s infinite linear;
  -webkit-border-radius: 50%;
  border-radius: 50%;
  border: 1px solid lightseagreen;
}
.loader .dot:nth-of-type(1) {
  -webkit-animation-delay: 0.15s;
  -moz-animation-delay: 0.15s;
  -o-animation-delay: 0.15s;
  -ms-animation-delay: 0.15s;
  animation-delay: 0.15s;
}
.loader .dot:nth-of-type(2) {
  animation-delay: 0.3s;
}
.loader .dot:nth-of-type(3) {
  animation-delay: 0.45s;
}
.loader .dot:nth-of-type(4) {
  animation-delay: 0.6s;
}
.loader .dot:nth-of-type(5) {
  animation-delay: 0.75s;
}
.loader .dot:nth-of-type(6) {
  animation-delay: 0.9s;
}
.loader .dot:nth-of-type(7) {
  animation-delay: 1.05s;
}
.loader .cogs {
  transform: scale(0.8);
}
.loader .cogs .cog {
  animation: rotate1 2s infinite;
  animation-timing-function: linear;
  background: green;
  -webkit-border-radius: 50%;
  border-radius: 50%;
  width: 100px;
  height: 100px;
  position: absolute;
  top: 40px;
  left: 125px;
}
.loader .cogs .cog.cog0 {
  animation: rotate2 2s infinite;
  animation-timing-function: linear;
  transform: rotate(22.5deg);
  top: -41px;
  left: 206px;
}
.loader .cogs .cog.cog1 {
  animation: rotate2 2s infinite;
  animation-timing-function: linear;
  transform: rotate(22.5deg);
  top: -41px;
  left: 44px;
}
.loader .cogs .cog:after {
  content: '';
  position: absolute;
  top: 40px;
  left: 40px;
  right: 40px;
